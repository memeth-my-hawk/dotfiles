URxvt.font: xft:Source Code Pro:bold:size=9
Urxvt*allowBoldFonts: true
URxvt*loginShell: true
URxvt*vt100*geometry: 80x50
URxvt*saveLines: 123456
URxvt*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
URxvt*eightBitInput: false
URxvt*foreground: #ffffff
URxvt*background: #1a1c1d
! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white
!
URxvt*color0:    #262626
URxvt*color1:    #e60000
URxvt*color2:    #488466
URxvt*color3:    #999900
URxvt*color4:    #4d94ff
URxvt*color5:    #8000ff
URxvt*color6:    #33cccc
URxvt*color7:    #ffffff
!
! Light
! color8    black
! color9    red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white
!
URxvt*color8:    #595959
URxvt*color9:    #ff3333
URxvt*color10:    #5ba480
URxvt*color11:    #e6e600
URxvt*color12:    #0066ff
URxvt*color13:    #9933ff
URxvt*color14:    #1affff
URxvt*color15:    #e2e2e2
!
URxvt*boldMode: true
URxvt*colorBDMode: true
Urxvt*colorBD: rgb:fc/fc/fc
URxvt*scrollTtyOutput: false
